#include "holberton.h"

/* betty style doc for function main goes there */

/**
 *print alphabet - the funtion who have the commands inside
 *
 *the _putchar is a command made for this exercice declared in the holberton.h  *header
 * Return: Always 0 (Success)
 */

void print_alphabet(void)
{
	char a;
		for (a = 'a' ; a <= 'z' ; a++)
		{
			_putchar(a);
		}
	_putchar('\n');
}
